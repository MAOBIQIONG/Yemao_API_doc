            list:[
                {
                    product_id:"作品id"
                    user_id:"用户id"
                    product_pic:"作品图片",
                    vote_num:"投票数",
                    views_num:"浏览数"
                }
            ],
            pagination: {
                page: "当前页码",
                limit: "每页数据量",
                total: "总页数"
            }
        }

